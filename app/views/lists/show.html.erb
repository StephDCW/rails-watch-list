<h1><%= @list.name %></h1>
<br>
<br>
<ul>
<% @list.bookmarks.each do |b| %>
<li><%= b.movie[:title] %></li>
<li><%= b.movie[:overview] %></li>
<li><%= b.comment %></li>
<%= link_to "Delete", bookmark_path(b), method: :delete, class: 'btn btn-primary' %>
<br>
<% end %>
</ul>

<%= link_to "Create a bookmark", new_list_bookmark_path(@list), class: 'btn btn-primary' %>
<%= link_to "Back to the list list", lists_path, class: 'btn btn-primary' %>
